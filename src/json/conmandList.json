{
  "name": "List of commands",
  "help": [
    {
      "command": "help",
      "description": "Show list of commands and their usage"
    },
    {
      "command": "help <Command>",
      "description": "Get help for usage of <Command>"
    }
  ],
  "cd": [
    {
      "command": "cd",
      "description": "Get current directory path"
    },
    {
      "command": "cd ..",
      "description": "Change to the parrent directory of the current directory"
    },
    {
      "command": "cd <Path>",
      "description": "Change current directory to <Path>"
    }
  ],
  "time": [
    {
      "command": "time",
      "description": "Display current time"
    },
    {}
  ],
  "notepad": {
    "command": "notepad",
    "description": "Open Notepad",
    "mode": [
      "fore",
      "back"
    ]
  },
  "process": {
    "description": "Working with processes",
    "commands": [
      {
        "command": [
          "process -list",
          "process -l"
        ],
        "description": "Display list of processes"
      },
      {
        "command": [
          "process -stop <Id>",
          "process -s <Id>"
        ],
        "description": "Stop process with id <Id>"
      },
      {
        "command": [
          "process -resume <Id>",
          "process -r <Id>"
        ],
        "description": "Resume process with id <Id>"
      },
      {
        "command": [
          "process -kill <Id>",
          "process -k <Id>"
        ],
        "description": "Kill process with id <Id>"
      },
      {
        "command": [
          "process -kill -a",
          "process -k -all"
        ],
        "description": "Kill all process"
      }
    ]
  },
  "env": [
    {
      "command": "env <Path>",
      "description": "Display the value of the environment variable <Path>"
    },
    {
      "command": [
        "env -all",
        "env -a"
      ],
      "description": "Display all environment variables"
    },
    {
      "command": [
        "env -add <Path> <Value>",
        "env -a <Path> <Value>"
      ],
      "description": "Add the environment variable <Path> with its value <Value>"
    },
    {
      "command": [
        "env -del <Path>",
        "env -d <Path>"
      ],
      "description": "Add the environment variable <Path> with its value <Value>"
    }
  ],
  "history": [
    {
      "command": "history -show",
      "description": "Print all used commands"
    },
    {
      "command": [
        "history -del",
        "history -d"
      ],
      "description": "Delete command history"
    }
  ],
  "clear": {
    "command": "clear",
    "description": "Clear Tiny Shell"
  },
  "exit": {
    "command": "exit",
    "description": "exit TinyShell"
  }
}